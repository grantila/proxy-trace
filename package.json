{
  "name": "proxy-trace",
  "version": "0.0.0-development",
  "description": "Typesafe transportation-agnostic proxy parser",
  "author": "Gustaf Räntilä",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/grantila/proxy-trace/issues"
  },
  "homepage": "https://github.com/grantila/proxy-trace#readme",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "./node_modules/.bin/rimraf dist && ./node_modules/.bin/tsc -p .",
    "test": "node_modules/.bin/istanbul cover node_modules/.bin/_mocha --build --check-leaks -- dist/test",
    "buildtest": "npm run build && npm run test",
    "travis-deploy-once": "travis-deploy-once",
    "semantic-release": "semantic-release",
    "cz": "git-cz"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/grantila/proxy-trace.git"
  },
  "keywords": [
    "address",
    "proxy",
    "forwarded",
    "remote",
    "ip"
  ],
  "devDependencies": {
    "@types/mocha": "2.x",
    "@types/node": "8.x",
    "chai": "4.x",
    "coveralls": "2.x",
    "cz-conventional-changelog": "^2.1.0",
    "istanbul": "1.1.0-alpha.1",
    "mocha": "4.x",
    "rimraf": "2.x",
    "semantic-release": "^15.12.3",
    "travis-deploy-once": "^5.0.9",
    "typescript": "2.6.x"
  },
  "optionalDependencies": {
    "@types/node": "8.x"
  },
  "dependencies": {
    "proxy-addr": "2.x"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
